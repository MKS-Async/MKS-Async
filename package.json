        "karma-nested-reporter": "0.1.1",
        "karma-nyan-reporter": "0.0.43",
        "karma-phantomjs-launcher": "0.1.4",
        "karma-unicorn-reporter": "0.1.4",
        "mocha": "1.20.1",
        "supertest": "2.0.0",

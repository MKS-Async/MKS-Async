{
    "name": "FutureInSight",
    "version": "1.0.0",
    "description": "Future InSight: An Async Greenfield Project",
    "engines": {
        "node": "4.4.7",
        "npm": "3.10.6"
    },
    "scripts": {
        "start": "node server/server.js",
        "postinstall": "bower install && npm run grunt_build",
        "grunt_build": "grunt build",
        "grunt_watch": "grunt watch",
        "grunt_test": "grunt test",
        "mongo_reset_sample": "node server/models.sample.js",
        "mongo_local_daemon": "mkdir -p data/db && mongod --dbpath data/db",
        "mongo_shutdown": "killall -9 mongod",
        "mongo_cleanup": "rm -rf data/db",
        "mocha_test": "mocha --watch --reporter spec spec/",
        "lint": "eslint --fix public server"
    },
    "author": "Cindy Sun, Dana Li, Tim Shiu, Sergey Potashnikov",
    "license": "ISC",
